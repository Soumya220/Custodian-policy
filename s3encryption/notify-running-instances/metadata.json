{
  "policy": {
    "name": "notify-running-instances",
    "description": "Notifies customers about running instances at 6 PM to remind them to stop the instances.\n",
    "resource": "aws.ec2",
    "filters": [
      {
        "type": "value",
        "key": "State.Name",
        "value": "running"
      }
    ],
    "actions": [
      {
        "type": "notify",
        "to": [
          "soumya.de@nagarro.com"
        ],
        "template_file": "running-instance-template.j2",
        "template_format": "text",
        "transport": {
          "type": "sqs",
          "queue": "https://sqs.us-east-1.amazonaws.com/200024982256/My-Queue"
        }
      }
    ]
  },
